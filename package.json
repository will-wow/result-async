{
  "name": "result-async",
  "version": "1.1.0",
  "description": "Error handling",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "docs": "typedoc && cp -R assets/* docs/assets/ && cp favicon.ico docs",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "test": "jest",
    "lint": "npm run lint-tslint && npm run lint-prettier",
    "lint-tslint": "tslint -p . --config tslint.json",
    "lint-prettier": "prettier --list-different \"**/*.{ts,json,md}\"",
    "format": "npm run format-tslint && npm run format-prettier",
    "format-tslint": "tslint -p . --fix --config tslint.json",
    "format-prettier": "prettier --write \"**/*.{ts,json,md}\""
  },
  "author": "Will Ockelmann-Wagner",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/will-wow/result.git"
  },
  "engines": {
    "node": "^8.11.3"
  },
  "keywords": [
    "error",
    "either",
    "result",
    "functional",
    "async",
    "promise",
    "fantasy land"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.9",
    "@types/ramda": "^0.26.3",
    "codecov": "^3.2.0",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "ramda": "^0.26.1",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.3333"
  },
  "dependencies": {}
}
