{
  "name": "result-async",
  "version": "1.2.4",
  "description": "Error handling",
  "main": "dist/result-async.cjs.js",
  "module": "dist/result-async.js",
  "types": "dist/index.d.ts",
  "files": [
    "lib/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build": "rollup -c",
    "watch": "tsc -watch -p ./",
    "docs": "typedoc && cp -R assets/* docs/assets/ && cp favicon.ico docs",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "test": "jest",
    "lint": "npm run lint-tslint && npm run lint-prettier",
    "lint-tslint": "tslint -p . --config tslint.json",
    "lint-prettier": "prettier --list-different \"**/*.{ts,json,md}\"",
    "format": "npm run format-tslint && npm run format-prettier",
    "format-tslint": "tslint -p . --fix --config tslint.json",
    "format-prettier": "prettier --write \"**/*.{ts,json,md}\""
  },
  "author": "Will Ockelmann-Wagner",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/will-wow/result.git"
  },
  "engines": {
    "node": ">=8.11.3"
  },
  "keywords": [
    "error",
    "either",
    "result",
    "functional",
    "async",
    "promise",
    "fantasy land"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/ramda": "^0.26.16",
    "codecov": "^3.5.0",
    "cross-fetch": "^3.0.4",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ramda": "^0.26.1",
    "rollup": "^1.19.4",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslib": "^1.10.0",
    "tslint": "^5.13.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "rollup-plugin-typescript2": "^0.22.1"
  }
}
