{
  "defaultSeverity": "error",
  "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
  "rules": {
    "class-name": false,
    "no-unused-variable": [true, { "ignore-pattern": "^_" }],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["test-project"]
  }
}
